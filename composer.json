{
    "name": "atnic/laravel-email-verification",
    "description": "Laravel Email Verification Scaffolding",
    "keywords": ["framework", "laravel", "packages", "email", "verification"],
    "license": "MIT",
    "type": "package",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Atnic\\EmailVerification\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atnic\\EmailVerification\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
